		minimal_width 400
		body /
			<= Menu_links $mol_list
				rows <= menu_rows /
	-
	Details!id $mol_page
		minimal_width 600
		title <= question_title!id \
		event_top?val <=> event_front_up?val
		tools /
			<= Details_permalink!id $mol_link
