$mol_attach $mol_view
	items?val /string
	sub /
		<= Content $mol_row
			sub <= content /$mol_view
	Add $mol_button_open
		title <= attach_title \
		files?val <=> attach_new?val null
	Item!id $mol_button_minor
		click?event <=> item_drop!id?event null
		sub /
			<= Image!id $mol_image
				title \
				uri <= item_uri!id \
